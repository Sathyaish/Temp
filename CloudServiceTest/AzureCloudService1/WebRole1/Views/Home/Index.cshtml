@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>ASP.NET</h1>
    <p class="lead">ASP.NET is a free web framework for building great Web sites and Web applications using HTML, CSS and JavaScript.</p>
    <p><a href="http://asp.net" class="btn btn-primary btn-lg">Learn more &raquo;</a></p>
</div>

<div class="row">
    <div class="col-md-12">
        <h2>Important Data About Your Application</h2>

        <h6>Current Role Instance Id: <span>@ViewBag.RoleId</span></h6>
        <h6>Roles in the role environment: <span>@ViewBag.Roles.ToString()</span></h6>

    </div>
</div>